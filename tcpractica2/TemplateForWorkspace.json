						"cell_type": "code",
						"metadata": {},
						"source": [
							"var horoscope = spark.sqlContext.read.option(\"header\", \"true\").csv(\"abfss://fstcpractica2@dlaccounttcpractica2.dfs.core.windows.net/horoscope.csv\")\r\n",
							"horoscope.show()"
						],
						"outputs": [],
						"execution_count": 3
					},
