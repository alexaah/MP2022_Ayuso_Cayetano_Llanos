									"deleting": false
								}
							}
						},
						"source": [
							"var countries = spark.sqlContext.read.option(\"header\", \"true\").csv(\"abfss://fstcpractica2@dlaccounttcpractica2.dfs.core.windows.net/countryContinent.csv\")\r\n",
							"countries.show()"
						],
						"outputs": [],
						"execution_count": 4
					},
					{
						"cell_type": "code",
						"metadata": {
							"jupyter": {
								"source_hidden": false,
								"outputs_hidden": false
							},
							"nteract": {
